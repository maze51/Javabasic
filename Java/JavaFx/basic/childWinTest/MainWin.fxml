<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" prefHeight="300.0" prefWidth="500.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="basic.childWinTest.MainWinController">
   <children>
      <TableView fx:id="table">
        <columns>
          <TableColumn fx:id="idCol" prefWidth="84.0" text="ID" />
          <TableColumn fx:id="nameCol" prefWidth="112.0" text="이름" />
            <TableColumn fx:id="addrCol" prefWidth="173.0" text="주소" />
        </columns>
      </TableView>
      <Button fx:id="btnAdd" mnemonicParsing="false" onAction="#btnAddClicked" text="데이터추가" />
   </children>
</VBox>
